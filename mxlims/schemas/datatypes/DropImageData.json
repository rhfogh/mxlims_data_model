{
    "$schema": "https://json-schema.org/draft-07/schema",
    "description": "DropImage, containing image as attached data.",
    "title": "DropImageData",
    "type": "object",
    "allOf": [
        {
            "$ref": "../datatypes/DropImage.json"
        }
    ],
    "properties": {
        "data": {
            "type": "string",
            "description": "The image, UUencoded."
        },
        "url": {
            "type": "null",
            "description": "url values are not allowed in this class; must be null or absent"
        }
    },
    "required": [ "data" ]
}