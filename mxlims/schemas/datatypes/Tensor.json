{
    "$id": "https://mxlims.org/schemas/datatypes/Tensor.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "description": "Tensor",
    "title": "Tensor",
    "type": "object",
    "properties": {
        "eigenvalues": {
            "description": "Eigenvalues of tensor",
            "maxItems": 3,
            "minItems": 3,
            "items": {
                "type": "number"
            },
            "title": "Eigenvalues",
            "type": "array"
        },
        "eigenvectors": {
            "description": "Eigenvectors (3D unit vectors) of tensor, in same order as eigenvalues",
            "maxItems": 3,
            "minItems": 3,
            "items": {
                "items": {
                    "type": "number"
                },
                "maxItems": 3,
                "minItems": 3,
                "type": "array"
            },
            "title": "Eigenvectors",
            "type": "array"
        }
    },
    "required": [
        "eigenvalues",
        "eigenvectors"
    ],
    "additionalProperties": false
}
