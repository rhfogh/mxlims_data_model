{
    "$schema": "https://json-schema.org/draft-07/schema",
    "description": "Resolution cutoff criteria",
    "title": "ResolutionCutoffs",
    "type": "object",
    "properties": {
        "R_merge": {
            "type": "number",
            "minimum": 0.0,
            "title": "R(merge)",
            "description": "R merge"
        },
        "R_meas": {
            "type": "number",
            "minimum": 0.0,
            "title": "R_meas",
            "description": "R meas"
        },
        "R_pim": {
            "type": "number",
            "minimum": 0.0,
            "title": "R_pim",
            "description": "R pim"
        },
        "I_over_SigI": {
            "type": "number",
            "title": "I/SigI",
            "description": "Average I / sigma(I)"
        },
        "CC_one_half": {
            "type": "number",
            "minimum": -1.0,
            "maximum": -1.0,
            "title": "CC(1/2)",
            "description": "CC 1/2"
        },
        "CC_ano": {
            "type": "number",
            "minimum": -1.0,
            "maximum": -1.0,
            "title": "CC_ano",
            "description": "CC_ano"
        },
        "SigAno": {
            "type": "number",
            "title": "SigAno",
            "description": "SigAno"
        }
},
    "additionalProperties": false
}