{
    "$schema": "https://json-schema.org/draft-07/schema",
    "description": "Base class for MXLIMS Logistical Samples\n\ndescribing Sample containers and locations\n(from Dewars and Plates to drops, pins and crystals)",
    "title": "LogisticalSample Data",
    "type": "object",
    "allOf": [
        {
            "$ref": "../data/MxlimsObjectData.json"
        }
    ],
    "properties": {
        "name": {
            "type": "string",
            "description": "Human-readable name."
        },
        "acronym": {
            "description": "Acronym - short synonym for Sample",
            "title": "Acronym",
            "type": "string"
        }
    }
}
