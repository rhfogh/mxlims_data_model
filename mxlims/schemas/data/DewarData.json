    "title": "DewarData",
    "type": "object",
    "properties": {
        "barcode": {
            "type": "string",
            "description": "The dewar barcode or RFID code"
        },
        "trackingDevice": {
            "allOf": [
                {
                    "$ref": "../datatypes/TrackingDevice.json"
                }
            ],
