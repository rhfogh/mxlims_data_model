{
    "$schema": "https://json-schema.org/draft-07/schema",
    "description": "A crystallization plate, with typed JSON containment lists",
    "title": "Plate",
    "type": "object",
    "allOf": [
        {
            "$ref": "../db_model/Plate.json"
        }
    ],
    "properties": {
        "container":{
            "$ref": "../logical_model/PlateShipment.json"
        },
        "contents": {
            "type": "array",
            "items": {
                "$ref": "../logical_model/PlateWell.json"
            },
            "minItems": 1
        }
    }
}
